{
    "version": "1.0",
    "name": "vendor/root",
    "resources": {
        "/root": "res"
    },
    "config": {
        "factory": {
            "out": {
                "class": "Puli\\MyFactory",
                "file": "My/Factory.php"
            }
        },
        "repository": {
            "type": "filesystem",
            "path": "repository"
        }
    },
    "packages": {
        "vendor/package1": {
            "install-path": "package1/sub/path",
            "installer": "composer"
        },
        "vendor/package2": {
            "install-path": "package2",
            "installer": "composer"
        }
    }
}
